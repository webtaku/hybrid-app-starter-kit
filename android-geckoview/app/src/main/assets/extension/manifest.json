{
  "manifest_version": 2,
  "name": "GeckoView Bridge",
  "version": "1.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "nativebridge@hybridview"
    }
  },
  "permissions": [
    "geckoViewAddons",
    "nativeMessaging",
    "nativeMessagingFromContent"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}